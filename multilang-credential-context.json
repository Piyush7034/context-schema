{
  "@context": {
    "@version": 1.1,

    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "farmer": "https://piyush7034.github.io/context-schema/multilang-credential-context.json#",

    "type": "@type",

    "FarmerCredential": "farmer:FarmerCredential",
    "Farm": "farmer:Farm",
    "Crop": "farmer:Crop",
    "PostalAddress": "schema:PostalAddress",

    "authority": "farmer:authority",
    "idType": "farmer:idType",
    "language": "@language",
    "value": "@value",

    "fullName": {
      "@id": "schema:name",
      "@container": "@set"
    },
    "gender": {
      "@id": "schema:gender",
      "@container": "@set"
    },

    "farmerId": "schema:identifier",
    "phoneNumber": "schema:telephone",
    "dateOfBirth": { "@id": "schema:birthDate", "@type": "xsd:date" },

    "address": "schema:address",
    "village": "schema:addressLocality",
    "district": "schema:addressRegion",
    "state": "schema:addressRegion",

    "farm": "farmer:farm",
    "primaryCommodity": "farmer:primaryCommodity",
    "totalLandArea": "farmer:totalLandArea",
    "value": { "@id": "schema:value", "@type": "xsd:decimal" },
    "unit": "schema:unitText",
    "ownershipType": "farmer:ownershipType",

    "crops": { "@id": "farmer:crops", "@container": "@set" },
    "cropName": "schema:name",
    "season": "farmer:season"
  }
}
