{
    "@context": {
      "@version": 1.1,
      "schema": "https://schema.org/",
      "xsd": "http://www.w3.org/2001/XMLSchema#",
      "farmer": "https://piyush7034.github.io/context-schema/multilang-credential-context.json#",
      "id": "@id",
      "type": "@type",
      "FarmerCredential": "farmer:FarmerCredential",
      "Farm": "farmer:Farm",
      "Crop": "farmer:Crop",
      "PostalAddress": "schema:PostalAddress",
      "authority": "farmer:authority",
      "idType": "farmer:idType",
      "fullName": {
        "@id": "schema:name",
        "@container": "@set",
        "@context": {
          "language": "@language",
          "value": "@value"
        }
      },
      "gender": {
        "@id": "schema:gender",
        "@container": "@set",
        "@context": {
          "language": "@language",
          "value": "@value"
        }
      },
      "farmerId": "schema:identifier",
      "phoneNumber": "schema:telephone",
      "dateOfBirth": {
        "@id": "schema:birthDate",
        "@type": "xsd:date"
      },
      "address": {
        "@id": "schema:address",
        "@type": "PostalAddress",
        "@context": {
          "village": "farmer:village",
          "district": "farmer:district",
          "state": "farmer:state"
        }
      },
      "farm": {
        "@id": "farmer:farm",
        "@type": "Farm",
        "@context": {
          "primaryCommodity": "farmer:primaryCommodity",
          "ownershipType": "farmer:ownershipType",
          "totalLandArea": {
            "@id": "farmer:totalLandArea",
            "@context": {
              "value": {
                "@id": "schema:value",
                "@type": "xsd:decimal"
              },
              "unit": "schema:unitText"
            }
          },
          "crops": {
            "@id": "farmer:crops",
            "@container": "@set",
            "@context": {
              "cropName": "schema:name",
              "season": "farmer:season"
            }
          }
        }
      }
    }
  }
